<!doctype html>
<html lang="vi">

<head>
  <%- include('../layout/head'); %>
</head>

<body>
  <%- include('../layout/header'); %>

  <%- include('../layout/page-hero-unit', {
    featuredImage: "/images/product/hero-unit.jpg",
    heading: "Yến Sào Thất Sơn",
    subHeading: null,
    paragraph: "Yến Sào Sạch & Chất Lượng Cao",
    buttonText: "Xem sản phẩm"
  }); %>

  <div class="clearfix-56"></div>

  <div id="product-list">
    <div class="container">
      <div class="row gx-3 gy-3">
        <% products.forEach(function(product) { %>
          <div class="col-12 col-md-6">
            <%- include('../products/item', { product }); %>
          </div>
        <% }); %>
      </div>

      <div class="clearfix-32"></div>

      <%- include('../layout/pagination'); %>
    </div>
  </div>

  <div class="clearfix-56"></div>

  <%- include('../layout/footer'); %>
  <%- include('../layout/foot'); %>
</body>

</html>
